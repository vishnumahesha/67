  "scoring_calibration": {
    "target_distribution": {
      "mean": 5.5,
      "std": 1.2,
      "min_score": 0.0,
      "max_score": 10.0
    },
    "score_bands": {
      "exceptional": { "min": 8.5, "max": 10.0, "percentile_top": 2 },
      "very_high": { "min": 7.5, "max": 8.49, "percentile_top": 10 },
      "above_average": { "min": 6.5, "max": 7.49, "percentile_top": 30 },
      "average": { "min": 4.5, "max": 6.49, "percentile_top": 70 },
      "below_average": { "min": 3.5, "max": 4.49, "percentile_top": 90 },
      "low": { "min": 0.0, "max": 3.49, "percentile_top": 100 }
    },
    "zscore_to_score": {
      "method": "sigmoid_scaled",
      "formula": "score = 5.5 + (z * 1.2 * 1.5)",
      "clamp_min": 0.0,
      "clamp_max": 10.0,
      "notes": [
        "z=0 maps to 5.5 (mean)",
        "z=+1 maps to ~7.3",
        "z=-1 maps to ~3.7",
        "z=+2 maps to ~9.1 (clamped at 10)",
        "z=-2 maps to ~1.9 (clamped at 0)"
      ]
    },
    "anti_inflation_rules": [
      "Never give 9+ without exceptional measurements AND high confidence",
      "Never give 8+ without above-average measurements AND medium+ confidence",
      "Default to 5.5 when uncertain",
      "Round scores to 1 decimal place"
    ]
  },

  "dampen_low_quality": {
    "description": "When photo quality is low, dampen extreme scores toward mean",
    "method": "linear_interpolation_to_mean",
    "formula": "dampened_score = lerp(mean, raw_score, quality_factor)",
    "quality_thresholds": {
      "block_below": 0.35,
      "heavy_dampen_below": 0.50,
      "light_dampen_below": 0.70,
      "no_dampen_above": 0.70
    },
    "dampen_factors": {
      "quality_0.35_to_0.50": 0.4,
      "quality_0.50_to_0.60": 0.6,
      "quality_0.60_to_0.70": 0.8,
      "quality_above_0.70": 1.0
    },
    "confidence_reduction": {
      "quality_below_0.50": "low",
      "quality_0.50_to_0.70": "reduce_one_level",
      "quality_above_0.70": "no_change"
    },
    "notes": [
      "Dampening pulls extreme scores toward 5.5",
      "A raw 8.0 with quality 0.5 becomes: 5.5 + (8.0-5.5)*0.6 = 7.0",
      "A raw 3.0 with quality 0.5 becomes: 5.5 + (3.0-5.5)*0.6 = 4.0"
    ]
  },

