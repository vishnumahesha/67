  "confidence_mapping": {
    "description": "How to assign confidence levels based on measurement reliability and photo quality",
    "rules": [
      {
        "condition": "reliability=high AND quality>=0.7",
        "confidence": "high"
      },
      {
        "condition": "reliability=high AND quality>=0.5",
        "confidence": "medium"
      },
      {
        "condition": "reliability=medium AND quality>=0.7",
        "confidence": "medium"
      },
      {
        "condition": "reliability=medium AND quality>=0.5",
        "confidence": "low"
      },
      {
        "condition": "reliability=low OR quality<0.5",
        "confidence": "low"
      }
    ],
